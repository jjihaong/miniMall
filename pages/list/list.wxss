/* pages/list/list.wxss */
page{
  height: 100%;
}
.container{
  flex-direction: row;
  height: 100%;
}
.list-left{
  width: 150rpx;
  height: 100%;
  border-right: 1px solid #eee;
}
.list-item{
  font-size: 28rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  box-sizing: border-box;
  border-left: 2px solid #fff;
  padding: 4rpx 0;
}
.list-item.on{
  border-color: #AB956D;
  color: #AB956D;
}
.list-right{
  width: calc(100% - 150rpx);
  height: 100%;
}
.list-main{
  min-height: 100%;
  height: 100%;
}
.prev{
  width: 100%;
  height: 404rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.prev>image{
  max-width: 100%;
}
.info{
  width: 100%;
  text-align: center;
  margin-top: 24rpx;
}